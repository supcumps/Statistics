		Sub RefreshResults()
		  
		  ResultsListBox.RemoveAllRows
		  ResultsListBox.RemoveAllRows
		  ResultsListBox.ColumnCount = 1
		  ResultsListBox.HeaderAt(0) = header1
		  
		  If dataStrings = Nil Or dataStrings.Count = 0 Then
		    ResultsListBox.AddRow("No data to display.")
		    Return
		  End If
		  
		  For i As Integer = 0 To dataStrings.LastIndex
		    ResultsListBox.AddRow("Row " + Str(i + 1) + ": " + dataStrings(i))
		  Next
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub SetData(headerName As String, rawData() As String)
		  
		  Self.header1 = headerName
		  
		  Var filteredStrings() As String
		  Var numericValues() As Double
		  
		  For Each s As String In rawData
		    Var v As Double = Val(s)
		    If s.Trim <> "" And v <> 0 Then
		      filteredStrings.AddRow(s)
		      numericValues.AddRow(v)
		    End If
		  Next
		  
		  Self.dataStrings = filteredStrings
		  Self.dataDoubles = numericValues
		  
		  RefreshResults()
		End Sub
	#tag EndMethod


